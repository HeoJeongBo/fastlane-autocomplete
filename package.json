{
  "name": "fastlane-autocomplete",
  "displayName": "Fastlane Autocomplete",
  "description": "Fastfile 작성을 위한 자동완성 및 신택스 하이라이팅 확장",
  "version": "0.1.0",
  "publisher": "heojeongbo",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "keywords": [
    "fastlane",
    "fastfile",
    "autocomplete",
    "syntax highlighting",
    "ios",
    "android"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:fastfile"
  ],
  "contributes": {
    "languages": [
      {
        "id": "fastfile",
        "aliases": ["Fastfile", "fastfile"],
        "extensions": [".Fastfile"],
        "filenames": ["Fastfile"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fastfile",
        "scopeName": "source.fastfile",
        "path": "./syntaxes/fastfile.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "bun run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build": "bun run compile",
    "dev": "bun run watch"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}